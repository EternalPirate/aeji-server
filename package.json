{
  "name": "node-js-getting-started",
  "version": "0.3.0",
  "description": "A sample Node.js app using Express 4",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src",
    "server:prod": "node ./dist",
    "dev": "npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "firebase-admin": "7.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "socket.io-client": "^2.2.0"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "node-pre-gyp": "^0.13.0",
    "nodemon": "^1.19.0"
  }
}
